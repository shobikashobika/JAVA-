public class Main {
    public static void main(String args[]) {
        int a[] = {1,2,3,4,5};
        System.out.println("original array");
        for(int i:a)
        System.out.println(i);
     int b[] = a.clone();
     System.out.println("cloned Array");
     for(int i:b)
     System.out.println(i);
      System.out.print("Are they equal?");
       System.out.print(a==b);
    }
}
