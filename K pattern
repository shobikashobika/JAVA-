public class Main {
    public static void main(String[] args) {
        for (char c = 'E'; c >= 'A'; c--) {
            for (char i = 'A'; i <= c; i++) {
                System.out.print(i);
            }
                System.out.println();
            }
                for (char c = 'A'; c <= 'E'; c++) {
            for (char i = 'A'; i <= c; i++) {
                System.out.print(i);
            }
            System.out.println();
        }
    }
  }

